{
	"packages":{
		"testbus":{
			"base_dir":".",
			"resources":[],
			"templates":[],
			"includes":[],
			"commands":[],
			"services":{
				"middlewares":{
					"type":"middlewares_svc"
				},
				"metrics":{
					"type":"metrics_svc"
				},
				"resources":{
					"type":"resources_svc"
				},
				"messaging":{
					"type":"messages_svc"
				},
				"topology":{
					"type":"topology_svc"
