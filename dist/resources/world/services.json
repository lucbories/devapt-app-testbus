{
	"services":{
		"authentication":{
			"type":"security",
			"routes":[
				{
					"route":"/login",
					"action":"login"
				},
				{
					"route":"/signin",
					"action":"login"
				},
				{
					"route":"/logout",
					"action":"logout"
				},
				{
					"route":"/signup",
					"action":"signup"
				},
				{
					"route":"/renew",
					"action":"renew"
				}
			]
		},

		"html_assets_1":{
			"type":"html_assets",
			"routes":[
				{
					"is_global":true,
					"route":"/",
					"directory":"D:/DATAS/GitHub/devapt-app-devtools/dist/assets",
					"default":"index.html"
				},

				{
					"route":"/css/.*.css",
					"directory":"./public/assets/css"
				},
				{
					"route":"/js/.*.js",
					"directory":"public/assets/js"
				},
				{
					"route":"/img",
					"directory":"./public/assets/img"
				},
				{
					"route":"/plugins",
					"plugins":["Foundation6", "Labs", "Cytoscape", "Devtools", "Sparklines"]
				}
			],
			"security":{
				"enabled":false,
				"authentication":{
					"enabled":false,
					"engines":[]
				},
				"authorization":{
					"enabled":false,
					"engines":[],
					"permissions":{
						"*":["*"]
					}
				}
			}
		},
		
		"rest_api_models_query_1":{ 
			"type":"rest_api_models_query",
			"routes":[
				{
					"route":"/crud"
				}
			]
		},
		
